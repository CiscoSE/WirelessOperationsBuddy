{
  "manifest_version": 3,
  "name": "Wireless Operations Buddy",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "wifi.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "webRequest",
    "storage"
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
}